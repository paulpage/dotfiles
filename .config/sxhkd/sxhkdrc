# Programs
super + Return
	$TERMINAL
super + d
	dmenu_run
super + e
	pcmanfm-qt
super + a
	pavucontrol
super + i
	$BROWSER
super + c
    $TERMINAL -e calc


super + shift + x
    sudo shutdown now

XF86AudioLowerVolume
    pulsemixer --change-volume -5

XF86AudioRaiseVolume
    pulsemixer --change-volume +5

